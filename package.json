{
  "name": "mongo-func",
  "version": "0.0.16",
  "description": "A functional approach to MongoDB",
  "keywords": [
    "mongo",
    "mongodb",
    "function",
    "functional",
    "curry",
    "database"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "lint": "./node_modules/.bin/eslint src/* && ./node_modules/.bin/eslint test/*",
    "test": "npm run build && npm run lint && mocha",
    "security": "./node_modules/nsp/bin/nsp check"
  },
  "engines": {
    "node": ">= 0.10.x"
  },
  "dependencies": {
    "mongodb": "2.2.12",
    "ramda": "0.21.0",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "blanket": "~1.1.7",
    "coveralls": "~2.11.4",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "10.1.0",
    "mocha-lcov-reporter": "0.0.2",
    "nsp": "^3.1.0",
    "prettier": "^1.8.1"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  },
  "author": {
    "name": "Stephen Scott",
    "email": "scottie1984@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:scottie1984/mongo-func.git"
  },
  "license": "BSD-2-Clause",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/scottie1984/mongo-func/issues"
  },
  "homepage": "https://github.com/scottie1984/mongo-func"
}
